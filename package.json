    "@babel/preset-env": "^7.8.6",
    "babel-jest": "^25.1.0",
    "cross-env": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-jest": "^23.8.0",
    "eslint-plugin-jest-formatting": "^1.2.0",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "superagent": "^5.2.2",
    "supertest": "^4.0.2"
