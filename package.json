  "bugs": {
    "url": "https://github.com/Mousticke/IdeaAppBackEnd/issues"
  },
  "homepage": "https://github.com/Mousticke/IdeaAppBackEnd#readme",
  "dependencies": {
    "@hapi/joi": "^17.1.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.9.2",
    "morgan": "^1.9.1",
    "passport": "^0.4.1",
